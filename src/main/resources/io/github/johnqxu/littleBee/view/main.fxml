<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<VBox fx:id="mainBox" prefHeight="320.0" prefWidth="642.0" xmlns="http://javafx.com/javafx/17"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:controller="io.github.johnqxu.littleBee.controller.MainController">
    <children>
        <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="320.0" prefWidth="615.0" VBox.vgrow="ALWAYS">
            <children>
                <Button fx:id="projectBtn" layoutX="49.0" layoutY="58.0" mnemonicParsing="false" prefHeight="109.0"
                        prefWidth="124.0"
                        text="选择培训项目表" onAction="#selectProjectExcel"/>
                <Button fx:id="employBtn" layoutX="255.0" layoutY="58.0" mnemonicParsing="false" prefHeight="109.0"
                        prefWidth="124.0"
                        text="选择人员花名册" onAction="#selectEmployExcel"/>
                <Button fx:id="projectEmployBtn" layoutX="480.0" layoutY="58.0" mnemonicParsing="false"
                        prefHeight="109.0" prefWidth="124.0"
                        text="选择项目签到表" onAction="#selectProjectEmployExcel"/>
                <ProgressBar fx:id="progressBar" layoutX="41.0" layoutY="230.0" prefHeight="18.0" prefWidth="562.0" progress="0.0"/>
                <Button fx:id="startBtn" layoutX="501.0" layoutY="263.0" mnemonicParsing="false" prefHeight="36.0"
                        prefWidth="102.0"
                        text="开始处理" onAction="#performAction"/>
            </children>
        </AnchorPane>
    </children>
</VBox>
